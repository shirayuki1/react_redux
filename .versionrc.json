{
  "header": "Changelog",
  "types": [
    { "type": "feat", "section": "Features" },
    { "type": "fix", "section": "Bug Fixes" },
    { "type": "chore", "hidden": true },
    { "type": "docs", "section": "Docs" },
    { "type": "style", "hidden": true },
    { "type": "refactor", "hidden": "Refactor" },
    { "type": "perf", "hidden": true },
    { "type": "test", "hidden": true }
  ],
  "commitUrlFormat": "https://github.com/jamolpe/front-end-base-template/commit/{{hash}}",
  "compareUrlFormat": "https://github.com/jamolpe/front-end-base-template/compare/{{previousTag}}...{{currentTag}}",
  "issuePrefixes": ["#"],
  "issueUrlFormat": "https://github.com/jamolpe/front-end-base-template/issues/{{id}}"
}
